                            <RowDefinition Height = "20" />
                            <RowDefinition Height = "10" />

                            <RowDefinition Height = "20" />
                            <RowDefinition Height = "10" />

                            <RowDefinition Height = "20" />
                            <RowDefinition Height = "10" />

                            <RowDefinition Height = "20" />
                            <RowDefinition Height = "10" />

                            <RowDefinition Height = "20" />
                            <RowDefinition Height = "10" />
                            <RowDefinition Height = "60" />
                            <RowDefinition Height = "10" />

                            <RowDefinition Height = "*" />
                            <RowDefinition Height = "10" />
                        </Grid.RowDefinitions>

                        <ComboBox x:Name="CboSkills" Grid.Row="3" Grid.Column="1" Grid.ColumnSpan="5" materialDesign:HintAssist.Hint="Select skill" Style="{StaticResource MaterialDesignFloatingHintComboBox}"  
                                  ItemsSource="{x:Static local:MainWindow.Skills}" DisplayMemberPath="Name" SelectionChanged="CboSkills_SelectionChanged"/>
                        <Button x:Name="BtnRefreshPlot" Grid.Row="3" Grid.Column="7" Grid.ColumnSpan="6"  Content="{materialDesign:PackIcon Refresh}" HorizontalAlignment="Left" VerticalAlignment="Center" Click="BtnRefreshPlot_Click" />


