                                <DataGridTextColumn Header="Saturday" Width="*" Foreground="#009788" FontSize="15" Binding="{Binding Saturday, Mode=OneWay}"/>
                                <DataGridTextColumn Header="Sunday" Width="*" Foreground="#009788" FontSize="15" Binding="{Binding Sunday, Mode=OneWay}"/>
                            </DataGrid.Columns>
